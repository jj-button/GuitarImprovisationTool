                                        <TextBlock Grid.Row="3" HorizontalAlignment="Center" VerticalAlignment="Center" Background="#FF999999" FontFamily="Segoe UI Semibold">A</TextBlock>
                                        <TextBlock Grid.Row="5" HorizontalAlignment="Center" VerticalAlignment="Center" Background="#FF999999" FontFamily="Segoe UI Semibold">D</TextBlock>
                                        <TextBlock Grid.Row="7" HorizontalAlignment="Center" VerticalAlignment="Center" Background="#FF999999" FontFamily="Segoe UI Semibold">G</TextBlock>
                                        <TextBlock Grid.Row="9" HorizontalAlignment="Center" VerticalAlignment="Center" Background="#FF999999" FontFamily="Segoe UI Semibold">B</TextBlock>
                                        <TextBlock Grid.Row="11" HorizontalAlignment="Center" VerticalAlignment="Center" Background="#FF999999" FontFamily="Segoe UI Semibold">e</TextBlock>

                                    </Grid>
                                </Border>
                            </Grid>
                        </Border>

                        <Border Grid.Column="1" x:Name="KTPanel" BorderBrush="#000" BorderThickness="1px">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="10.5*"/>
                                    <ColumnDefinition Width="0.5*"/>
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="3*"/>
                                    <RowDefinition Height="4.0*"/>
                                    <RowDefinition Height="0.5*"/>
                                    <RowDefinition Height="2.1*"/>
                                    <RowDefinition Height="0.5*"/>
                                    <RowDefinition Height="4.0*"/>
                                    <RowDefinition Height="0.5*"/>
                                    <RowDefinition Height="1.5*"/>
                                    <RowDefinition Height="1.5*"/>
                                    <RowDefinition Height="2.9*"/>
                                </Grid.RowDefinitions>

                                <Border Background="#FF999999" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="1" BorderBrush="#000" BorderThickness="1px">
                                    <TextBlock Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White" FontSize="20" FontFamily="Segoe UI Light">Key</TextBlock>
                                </Border>

                                <Border Background="#FFF" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2" BorderBrush="#000" BorderThickness="1px">


                                    <ComboBox x:Name="KeySelectionBox" Background="#FFF" Opacity="1" VerticalAlignment="Stretch" BorderBrush="White" FlowDirection="LeftToRight">
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="A" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">A</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="A#" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">A#</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="B" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">B</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="C" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">C</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="C#" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">C#</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="D" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">D</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="D#" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">D#</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="E" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">E</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="F" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">F</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="F#" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">F#</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="G" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">G</TextBlock>
                                        </ComboBoxItem>
                                        <ComboBoxItem Background="#FFFAFAFA" Tag="G#" VerticalAlignment="Center">
                                            <TextBlock Foreground="Black" FontFamily="Segoe UI Light" FontSize="18">G#</TextBlock>
                                        </ComboBoxItem>
                                    </ComboBox>
                                </Border>

                                <Border Background="#FFF" Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="3" BorderBrush="#000" BorderThickness="0px">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>
                                        <Viewbox Grid.Column="0" HorizontalAlignment="Left">
                                            <RadioButton Grid.Column="0" x:Name="MajorRadio" Content="Major" FontFamily="Segoe UI Light" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" Checked="MajorRadio_Checked">
                                                <RadioButton.RenderTransform>
                                                    <TransformGroup>
                                                        <ScaleTransform ScaleX="1"/>
                                                        <SkewTransform/>
                                                        <RotateTransform/>
                                                        <TranslateTransform/>
                                                    </TransformGroup>
                                                </RadioButton.RenderTransform>
                                            </RadioButton>
                                        </Viewbox>
                                        <Viewbox Grid.Column="1" HorizontalAlignment="Left">
                                            <RadioButton Grid.Column="1" x:Name="MinorRadio" Content="Minor" FontFamily="Segoe UI Light" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" Checked="MinorRadio_Checked">
                                                <RadioButton.RenderTransform>
                                                    <TransformGroup>
                                                        <ScaleTransform ScaleX="1"/>
                                                        <SkewTransform/>
                                                        <RotateTransform/>
                                                        <TranslateTransform/>
                                                    </TransformGroup>
                                                </RadioButton.RenderTransform>
                                            </RadioButton>
                                        </Viewbox>
                                    </Grid>
                                </Border>



                                <TextBlock x:Name="KTEnterDialogue" Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="3" VerticalAlignment="Center" Text="" Foreground="Green" />

                                <Button x:Name="KTEnter" Background="#FF999999" Grid.Column="1" Grid.Row="6" Grid.RowSpan="3" Grid.ColumnSpan="3" Click="KTEnter_Click">
                                    <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White" FontSize="14" FontFamily="Segoe UI Light">Enter</TextBlock>
                                </Button>

                            </Grid>
                        </Border>

                    </Grid>
                </Border>
            </Grid>
        </Border>

    </Grid>
</Window>
