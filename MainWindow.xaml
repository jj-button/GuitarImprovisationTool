<Window x:Class="NEA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NEA"
        mc:Ignorable="d"
        Title="MainWindow" Height="675" Width="1200"
        WindowStyle="None">
    <Grid x:Name="MainGrid">
        <Border x:Name="ToolWindowContainer">
            <Grid x:Name="ToolWindowGrid">
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.5*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="15*"/>
                    <RowDefinition Height="8*"/>
                </Grid.RowDefinitions>

                <Border Grid.Row="0" x:Name="WindowBarContainer" Background="#FF999999">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="3*"/>
                            <ColumnDefinition Width="0.1*"/>
                        </Grid.ColumnDefinitions>

                        <TextBlock Padding="8px" VerticalAlignment="Center" Foreground="White" FontSize="11" FontFamily="Segoe UI Light">Guitar Improvisation Tool</TextBlock>
                        <Button x:Name="ExitButton" Grid.Column="2" Click="CloseApp">
                            <TextBlock Padding="8px" Grid.Column="2" VerticalAlignment="Center" Foreground="White" FontSize="11" FontFamily="Segoe UI Semibold" Text="X" HorizontalAlignment="Center" RenderTransformOrigin="0.5,0.5">
                                <TextBlock.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="2"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </TextBlock.RenderTransform>
                            </TextBlock>
                        </Button>
                    </Grid>
                </Border>

                <Border Grid.Row="1" x:Name="SettingsBarContainer">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="17*"/>
                            <ColumnDefinition Width="8*"/>
                            <ColumnDefinition Width="0.25*"/>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="0.25*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="0.5*"/>
                        </Grid.ColumnDefinitions>

                        <Border Grid.Column="0" Grid.ColumnSpan="8"  Background="#FFEAEAEA" >

                        </Border>

                        <TextBlock x:Name="SettingsDialogue" Grid.Column="1" FontFamily="Segoe UI Light" VerticalAlignment="Center" FontSize="15px" Text="" TextAlignment="Right" Foreground="Green"/>

                        <TextBlock x:Name="TempoBlock" Grid.Column="3" FontFamily="Segoe UI Light" VerticalAlignment="Center" FontSize="20px" HorizontalAlignment="Center">Tempo:</TextBlock>

                        <TextBox x:Name="TempoBox" Grid.Column="4" FontFamily="Segoe UI Light" VerticalAlignment="Center" FontSize="20px" BorderBrush="#FFEAEAEA" SelectionBrush="#FFEAEAEA" TextAlignment="Center"/>

                        <TextBlock Grid.Column="6" x:Name="PlayButton" MouseLeftButtonDown="PlayButton_MouseLeft">
                            <TextBlock.Background>
                                <ImageBrush ImageSource="./assets/playButton.jpg"/>
                            </TextBlock.Background>
                        </TextBlock>

                        <DockPanel Grid.Column="0">
                            <Menu DockPanel.Dock="Left" FontFamily="Segoe UI Light" FontSize="16" VerticalAlignment="Stretch" Margin="0,0,0,0">
                                <MenuItem Header="   File   " BorderBrush="Gray" VerticalAlignment="Top" FontSize="19.75">
                                    <MenuItem Header="Save Tab" FontSize="12" Click="SaveTab_Click">
                                        <MenuItem.Icon>
                                            <Image Source="./assets/saveIcon.jpg"/>
                                        </MenuItem.Icon>
                                    </MenuItem>
                                </MenuItem>
                                <MenuItem Header="   Tab   " BorderBrush="Gray" VerticalAlignment="Top" FontSize="19.75">
                                    <MenuItem Header="Revert Opacity" FontSize="12" Click="RevertOpacity_Click">
                                        <MenuItem.Icon>
                                            <Image Source="./assets/revertOpacityIcon.JPG"/>
                                        </MenuItem.Icon>
                                    </MenuItem>
                                    <MenuItem Header="Clear Tab" FontSize="12" Click="ClearTab_Click">
                                        <MenuItem.Icon>
                                            <Image Source="./assets/revertOpacityIcon.JPG"/>
                                        </MenuItem.Icon>
                                    </MenuItem>
                                    <MenuItem Header="Clear Tab Preferences" FontSize="12">
                                        <MenuItem x:Name="ClearTab_E" Header="E" IsCheckable="True" IsChecked="True" FontSize="12" StaysOpenOnClick="True"/>
                                        <MenuItem x:Name="ClearTab_A" Header="A" IsCheckable="True" IsChecked="True" FontSize="12" StaysOpenOnClick="True"/>
                                        <MenuItem x:Name="ClearTab_D" Header="D" IsCheckable="True" IsChecked="True" FontSize="12" StaysOpenOnClick="True"/>
                                        <MenuItem x:Name="ClearTab_G" Header="G" IsCheckable="True" IsChecked="True" FontSize="12" StaysOpenOnClick="True"/>
                                        <MenuItem x:Name="ClearTab_B" Header="B" IsCheckable="True" IsChecked="True" FontSize="12" StaysOpenOnClick="True"/>
                                        <MenuItem x:Name="ClearTab_e" Header="e" IsCheckable="True" IsChecked="True" FontSize="12" StaysOpenOnClick="True"/>
                                    </MenuItem>
                                </MenuItem>
                            </Menu>
                        </DockPanel>
                    </Grid>
                </Border>

